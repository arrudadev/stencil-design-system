{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "cache": false,
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    }
  }
}
