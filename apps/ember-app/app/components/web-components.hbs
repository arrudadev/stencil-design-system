<main class="app">
  <my-heading>Ember + Web components</my-heading>

  <my-text>Type your name:</my-text>

  <my-input
    placeholder="Type..."
    {{on "changeInput" this.changeName}}
  ></my-input>

  <div style="margin-top: 10px;">
    <my-component name={{this.name}}></my-component>
  </div>
</main>